@model IEnumerable<E_LearningProject.Models.Category>

@{
    ViewData["Title"] = "Details";
    
}

<h1>Details</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Category-name
            </th>
            <th>
               
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Courses)
                </td>
            </tr>
        }
    </tbody>
</table>

